if (planificationFamiliale)
	//- VARIABLES
	-var title = 'Planification familiale', id = 'planificationFamiliale'
	//- LAYOUTS
	.col-12
		.card
			.card-body
				.d-flex.align-items-center.justify-content-between
					.d-flex.align-items-center.fw-bold
						ion-icon.text-1.fs-6.me-2(name='people-circle-outline')
						span.fs-7.text-dark= title
					//- download pdf / excel
					.btn-group.shadow-0(role='group')
						button.btn.btn-sm.btn-light(type='button' data-bs-toggle='dropdown' aria-expanded='false')
							<ion-icon class="fs-5" name="download-outline"></ion-icon>
						ul.dropdown-menu.shadow.border
							li
								button.dropdown-item.downloadSortiePdf(type='button' data-id=id data-region=document.region data-province=document.province data-csr=document.csr data-category=document.category data-trimestre=trimestre data-createdat=data.sortie ? data.sortie.createdAt : data.trimestre.createdAt data-updatedat= data.sortie ? data.sortie.updatedAt : '' data-submit=data.sortie ? data.sortie.submit : '')
									.d-flex.align-items-center
										img.me-2(src="/../../image/pdf.svg", alt="pdf" width="20px")
										span PDF
							li
								button.dropdown-item.downloadExcel(type='button' data-title=title data-id=id)
									.d-flex.align-items-center
										img.me-2(src="/../../image/excel.svg", alt="pdf" width="20px")
										span Excel
			.card-body
				.table-responsive
					table.table.thisTable.table-bordered(id=id data-title=title data-id=id data-align='center')
						tbody
							tr
								th(colspan="2") Pilule
								th(colspan="2") Progestatifs Injectables
								th(colspan="2") DIU
								th(colspan="2") condom
								th(colspan="2") Références
							tr
								th NA
								th AA
								th NA
								th AA
								th NA
								th AA
								th NA
								th AA
								th DIU
								th LT
							tr
								td= planificationFamiliale.pilule.na
								td= planificationFamiliale.pilule.aa
								td= planificationFamiliale.injectable.na
								td= planificationFamiliale.injectable.aa
								td= planificationFamiliale.diu.na
								td= planificationFamiliale.diu.aa
								td= planificationFamiliale.condom.na
								td= planificationFamiliale.condom.aa
								td= planificationFamiliale.reference.diu
								td= planificationFamiliale.reference.lt
			.card-body
				span.badge.bg-5.fw-normal
					.d-flex.align-items-center
						ion-icon.fs-5.me-2(name='information-circle-outline')
						.d-flex.flex-column.align-items-start
							span.fs-8.mb-2 NA: Nouvelles acceptantes
							span.fs-8.mb-2 AA: Anciennes acceptantes
							span.fs-8.mb-2 DIU: Dispositif Intra Utérin
							span.fs-8 LT: Ligature Tubaire